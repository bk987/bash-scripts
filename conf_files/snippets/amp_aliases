alias apastr="sudo service apache2 start"
alias apastp="sudo service apache2 stop"
alias apares="sudo service apache2 restart"
alias aparel="sudo service apache2 reload"
alias apasts="sudo service apache2 status"
alias phpstr="sudo service php7.3-fpm start"
alias phpstp="sudo service php7.3-fpm stop"
alias phpres="sudo service php7.3-fpm restart"
alias phprel="sudo service php7.3-fpm reload"
alias phpsts="sudo service php7.3-fpm status"
alias msqstr="sudo service mysql start"
alias msqstp="sudo service mysql stop"
alias msqres="sudo service mysql restart"
alias msqsts="sudo service mysql status"